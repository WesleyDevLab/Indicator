<%= simple_form_for @trade do |f| %>
  <%= f.input :underlying %>
  <%= f.input :expiration %>
  <%= f.input :option_type %>
  <%= f.input :short_strike_price %>
  <%= f.input :long_strike_price %>
  <%= f.button :submit %>
<% end %>
<br/>
<br/>

  

    <div id="table_div"></div>
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
      <script type="text/javascript">
      google.charts.load('current', {'packages':['table']});
      google.charts.setOnLoadCallback(drawTable);

      function drawTable() {
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Underlying');
        data.addColumn('string', 'Expiration');
        data.addColumn('string', 'Type');
        data.addColumn('number', 'Short Strike Price');
        data.addColumn('number', 'Long Strike Price');
        data.addRows(gon.trades);

        var table = new google.visualization.Table(document.getElementById('table_div'));

        table.draw(data, {showRowNumber: true, width: '100%', height: '100%'});
      }
    </script>